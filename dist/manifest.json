{
  "name": "WebtoonTrackr",
  "description": "Track webtoon updates!",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "popup.html",
      "default_title": "Open the popup"
  },
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
      "16": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
  }
}